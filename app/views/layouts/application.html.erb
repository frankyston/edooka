<!DOCTYPE html>
<html>
  <head>
    <title>Edooka - Building schools</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application'%>
    <%= javascript_include_tag 'application'%>
  </head>

  <body>
  
    <% flash.each do |name, msg| %>
    	<%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>

    <%= yield %>
  </body>
</html>
